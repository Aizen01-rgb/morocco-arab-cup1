        return self.cipher.encrypt(data).decode()
    
    def decrypt_data(self, encrypted_data):
        """ÙÙƒ ØªØ´ÙÙŠØ± Ø§Ù„Ø¨ÙŠØ§Ù†Ø§Øª"""
        return self.cipher.decrypt(encrypted_data.encode()).decode()
    
