            '/admin/',
            '/wp-content/',
            '/media/'
        ]
        
        for directory in test_dirs:
            url = urljoin(self.target_url, directory)
            
            time.sleep(random.uniform(0.5, 1))
            
            response = self.make_request(url)
            if response and response.status_code == 200:
                # ØªØ­Ù‚Ù‚ Ø¥Ø°Ø§ ÙƒØ§Ù†Øª ØµÙØ­Ø© ØªØµÙØ­ Ù…Ø¬Ù„Ø¯Ø§Øª
                indicators = ['Index of', 'Directory listing for', '<title>Index of']
                if any(indicator in response.text for indicator in indicators):
                    self.vulnerabilities.append({
                        'id': 'DIRECTORY-LISTING',
                        'name': 'Directory Listing Enabled',
                        'severity': 'MEDIUM',
                        'description': 'ÙŠÙ…ÙƒÙ† ØªØµÙØ­ Ù…Ø­ØªÙˆÙŠØ§Øª Ø§Ù„Ù…Ø¬Ù„Ø¯Ø§Øª',
                        'details': f'Directory: {directory}',
                        'fix': 'Disable directory listing in server configuration',
                        'impact': 'Information disclosure of file structure'
                    })
                    break
        
        self.log_encrypted("âœ… Directory listing check completed")
    
    def check_cms_versions(self):
        """ÙØ­Øµ Ø¥ØµØ¯Ø§Ø±Ø§Øª CMS ÙˆØ§Ù„Ø¥Ø¶Ø§ÙØ§Øª"""
        self.log_encrypted("ğŸ” Checking for CMS and versions...")
        
        cms_signatures = {
            'WordPress': {
                'paths': ['/wp-admin/', '/wp-content/', '/wp-includes/'],
                'version_patterns': ['content="WordPress', 'generator" content="WordPress']
            },
            'Joomla': {
                'paths': ['/media/jui/', '/administrator/', '/components/com_'],
                'version_patterns': ['generator" content="Joomla']
            },
            'Drupal': {
                'paths': ['/sites/default/', '/core/misc/', '/modules/'],
                'version_patterns': ['generator" content="Drupal']
            },
            'Magento': {
                'paths': ['/skin/frontend/', '/media/', '/js/mage/'],
                'version_patterns': ['Magento']
            }
        }
        
        try:
            response = self.make_request(self.target_url)
            if not response:
                return
            
            content = response.text
            
            for cms, info in cms_signatures.items():
                # ØªØ­Ù‚Ù‚ Ù…Ù† Ø§Ù„Ù…Ø³Ø§Ø±Ø§Øª
                for path in info['paths']:
                    if path in self.target_url or path in content:
                        self.scan_results['cms_detected'] = cms
                        
                        # Ø§Ù„Ø¨Ø­Ø« Ø¹Ù† Ø§Ù„Ø¥ØµØ¯Ø§Ø±
                        for pattern in info['version_patterns']:
                            if pattern in content:
                                # Ø§Ø³ØªØ®Ø±Ø§Ø¬ Ø±Ù‚Ù… Ø§Ù„Ø¥ØµØ¯Ø§Ø±
                                import re
                                version_match = re.search(r'(\d+\.\d+(\.\d+)?)', content[content.find(pattern):content.find(pattern)+100])
                                if version_match:
                                    self.scan_results['cms_version'] = version_match.group(1)
                                    self.log_encrypted(f"Detected {cms} version {version_match.group(1)}")
                                    
                                    # ØªØ­Ø°ÙŠØ± Ø¥Ø°Ø§ ÙƒØ§Ù† Ø§Ù„Ø¥ØµØ¯Ø§Ø± Ù‚Ø¯ÙŠÙ…Ø§Ù‹
                                    if cms == 'WordPress' and version_match.group(1):
                                        try:
                                            version_num = float(version_match.group(1).split('.')[0])
                                            if version_num < 6:
                                                self.vulnerabilities.append({
                                                    'id': 'OUTDATED-CMS',
                                                    'name': 'Outdated CMS Version',
                                                    'severity': 'HIGH',
                                                    'description': f'Ø¥ØµØ¯Ø§Ø± {cms} Ù‚Ø¯ÙŠÙ…',
                                                    'details': f'Version: {version_match.group(1)}',
                                                    'fix': f'Update {cms} to latest version',
                                                    'impact': 'Security vulnerabilities in old versions'
                                                })
                                        except:
                                            pass
                                    
                                break
                        break
        
        except Exception as e:
            self.log_encrypted(f"âŒ CMS detection error: {str(e)[:50]}")
        
        self.log_encrypted("âœ… CMS detection completed")
    
    def run_stealth_scan(self):
        """ØªØ´ØºÙŠÙ„ ÙØ­Øµ Ø®ÙÙŠ Ø´Ø§Ù…Ù„"""
        self.log_encrypted("ğŸš€ Starting stealth security scan")
        self.log_encrypted(f"Target: {self.target_url}")
        self.log_encrypted(f"Session ID: {self.session_id}")
        self.log_encrypted("=" * 60)
        
        start_time = time.time()
        
        # Ù‚Ø§Ø¦Ù…Ø© Ø§Ù„Ù…Ù‡Ø§Ù… Ù„Ù„ÙØ­Øµ
        scan_tasks = [
            ('Security Headers', self.check_security_headers),
            ('SSL/TLS Configuration', self.check_ssl_tls),
            ('Sensitive Files', self.check_sensitive_files),
            ('Directory Listing', self.check_directory_listing),
            ('SQL Injection', self.check_sql_injection),
            ('XSS Vulnerabilities', self.check_xss_vulnerabilities),
            ('CMS Detection', self.check_cms_versions)
        ]
        
        # ØªØ´ØºÙŠÙ„ Ø§Ù„Ù…Ù‡Ø§Ù… Ù…Ø¹ ØªØ£Ø®ÙŠØ±Ø§Øª Ø¹Ø´ÙˆØ§Ø¦ÙŠØ©
        for task_name, task_func in scan_tasks:
            self.log_encrypted(f"ğŸ” Running: {task_name}")
            
            # ØªØ£Ø®ÙŠØ± Ø¹Ø´ÙˆØ§Ø¦ÙŠ Ø¨ÙŠÙ† Ø§Ù„Ù…Ù‡Ø§Ù…
            time.sleep(random.uniform(2, 5))
            
            try:
                task_func()
            except Exception as e:
                self.log_encrypted(f"âš ï¸ Task {task_name} failed: {str(e)[:50]}")
        
        # Ø­Ø³Ø§Ø¨ ÙˆÙ‚Øª Ø§Ù„ÙØ­Øµ
        scan_duration = time.time() - start_time
        
        # ØªÙˆÙ„ÙŠØ¯ Ø§Ù„ØªÙ‚Ø±ÙŠØ±
        report = self.generate_report(scan_duration)
        
        self.log_encrypted("=" * 60)
        self.log_encrypted(f"âœ… Scan completed in {scan_duration:.2f} seconds")
        self.log_encrypted(f"ğŸ“Š Vulnerabilities found: {len(self.vulnerabilities)}")
        self.log_encrypted(f"ğŸ” Session ID: {self.session_id}")
        self.log_encrypted("âš ï¸ All logs and data are encrypted")
        
        return report
    
    def generate_report(self, scan_duration):
        """ØªÙˆÙ„ÙŠØ¯ ØªÙ‚Ø±ÙŠØ± Ø§Ù„ÙØ­Øµ"""
        # Ø­Ø³Ø§Ø¨ Ø¯Ø±Ø¬Ø© Ø§Ù„Ø£Ù…Ø§Ù†
        severity_scores = {'CRITICAL': 10, 'HIGH': 7, 'MEDIUM': 4, 'LOW': 1}
        total_score = 100
        
        for vuln in self.vulnerabilities:
            total_score -= severity_scores.get(vuln['severity'], 0)
        
        total_score = max(0, min(100, total_score))
        
        # ØªØ­Ø¯ÙŠØ¯ Ù…Ø³ØªÙˆÙ‰ Ø§Ù„Ø£Ù…Ø§Ù†
        if total_score >= 80:
            risk_level = "LOW ğŸŸ¢"
        elif total_score >= 60:
            risk_level = "MEDIUM ğŸŸ¡"
        elif total_score >= 40:
            risk_level = "HIGH ğŸŸ "
        else:
            risk_level = "CRITICAL ğŸ”´"
        
        report = {
            'scan_info': {
                'target': self.target_url,
                'scan_date': datetime.now().isoformat(),
                'scan_duration': round(scan_duration, 2),
                'session_id': self.session_id,
                'scanner_version': 'Anonymous Pro 3.0'
            },
            'summary': {
                'security_score': total_score,
                'risk_level': risk_level,
                'total_vulnerabilities': len(self.vulnerabilities),
                'vulnerabilities_by_severity': {
                    'CRITICAL': len([v for v in self.vulnerabilities if v['severity'] == 'CRITICAL']),
                    'HIGH': len([v for v in self.vulnerabilities if v['severity'] == 'HIGH']),
                    'MEDIUM': len([v for v in self.vulnerabilities if v['severity'] == 'MEDIUM']),
                    'LOW': len([v for v in self.vulnerabilities if v['severity'] == 'LOW'])
                }
            },
            'vulnerabilities': self.vulnerabilities,
            'cms_info': {
                'detected': self.scan_results.get('cms_detected', 'Not detected'),
                'version': self.scan_results.get('cms_version', 'Unknown')
            },
            'ssl_info': {
                'cipher': self.scan_results.get('ssl_cipher', 'Unknown')
            },
            'recommendations': self.generate_recommendations(),
            'disclaimer': '''
            âš ï¸ DISCLAIMER:
            This report is for educational and ethical security testing purposes only.
            Unauthorized scanning of websites is illegal.
            Always obtain proper authorization before testing any system.
            The scanner does not store any identifying information.
            All logs are encrypted and session-specific.
            '''
        }
        
        return report
    
    def generate_recommendations(self):
        """ØªÙˆÙ„ÙŠØ¯ ØªÙˆØµÙŠØ§Øª Ø¨Ù†Ø§Ø¡Ù‹ Ø¹Ù„Ù‰ Ø§Ù„Ù†ØªØ§Ø¦Ø¬"""
        recommendations = []
        
        if any(v['id'] == 'NO-HTTPS' for v in self.vulnerabilities):
            recommendations.append({
                'priority': 'HIGH',
                'action': 'Implement HTTPS',
                'description': 'Install SSL certificate and force HTTPS connections'
            })
        
        if any('SQL' in v['id'] for v in self.vulnerabilities):
            recommendations.append({
                'priority': 'CRITICAL',
                'action': 'Fix SQL Injection vulnerabilities',
                'description': 'Use prepared statements and input validation'
            })
        
        if any('XSS' in v['id'] for v in self.vulnerabilities):
            recommendations.append({
                'priority': 'HIGH',
                'action': 'Fix XSS vulnerabilities',
                'description': 'Implement input sanitization and output encoding'
            })
        
        if any('SENSITIVE-FILE' in v['id'] for v in self.vulnerabilities):
            recommendations.append({
                'priority': 'HIGH',
                'action': 'Secure sensitive files',
                'description': 'Restrict access to configuration and backup files'
            })
        
        if any('SEC-HEADERS' in v['id'] for v in self.vulnerabilities):
            recommendations.append({
                'priority': 'MEDIUM',
                'action': 'Add security headers',
                'description': 'Configure security headers in server settings'
            })
        
        # ØªÙˆØµÙŠØ§Øª Ø¹Ø§Ù…Ø©
        recommendations.extend([
            {
                'priority': 'MEDIUM',
                'action': 'Regular updates',
                'description': 'Keep all software and plugins updated'
            },
            {
                'priority': 'MEDIUM',
                'action': 'Backup regularly',
                'description': 'Maintain regular backups of website and database'
            },
            {
                'priority': 'LOW',
                'action': 'Security monitoring',
                'description': 'Implement security monitoring and logging'
            }
        ])
        
        return recommendations
    
    def save_report(self, report, format='json'):
        """Ø­ÙØ¸ Ø§Ù„ØªÙ‚Ø±ÙŠØ± ÙÙŠ Ù…Ù„Ù"""
        timestamp = datetime.now().strftime("%Y%m%d_%H%M%S")
        safe_url = self.target_url.replace('://', '_').replace('/', '_').replace('.', '_')
        filename = f"scan_report_{safe_url}_{timestamp}_{self.session_id[:8]}"
        
        if format == 'json':
            filepath = f"{filename}.json"
            with open(filepath, 'w', encoding='utf-8') as f:
                json.dump(report, f, indent=2, ensure_ascii=False)
            
            self.log_encrypted(f"ğŸ“„ Report saved: {filepath}")
            return filepath
        
        elif format == 'txt':
            filepath = f"{filename}.txt"
            with open(filepath, 'w', encoding='utf-8') as f:
                f.write("=" * 60 + "\n")
                f.write("SECURITY SCAN REPORT\n")
                f.write("=" * 60 + "\n\n")
                
                f.write(f"Target: {report['scan_info']['target']}\n")
                f.write(f"Date: {report['scan_info']['scan_date']}\n")
                f.write(f"Duration: {report['scan_info']['scan_duration']} seconds\n")
                f.write(f"Session ID: {report['scan_info']['session_id']}\n\n")
                
                f.write(f"Security Score: {report['summary']['security_score']}/100\n")
                f.write(f"Risk Level: {report['summary']['risk_level']}\n")
                f.write(f"Total Vulnerabilities: {report['summary']['total_vulnerabilities']}\n\n")
                
                f.write("Vulnerabilities:\n")
                f.write("-" * 40 + "\n")
                for i, vuln in enumerate(report['vulnerabilities'], 1):
                    f.write(f"{i}. [{vuln['severity']}] {vuln['name']}\n")
                    f.write(f"   Description: {vuln['description']}\n")
                    f.write(f"   Fix: {vuln['fix']}\n\n")
                
                f.write("\n" + report['disclaimer'])
            
            self.log_encrypted(f"ğŸ“„ Report saved: {filepath}")
            return filepath

# ==================== Ø§Ù„ÙˆØ§Ø¬Ù‡Ø© Ø§Ù„ØªÙØ§Ø¹Ù„ÙŠØ© ====================
def interactive_scanner():
    """ÙˆØ§Ø¬Ù‡Ø© ØªÙØ§Ø¹Ù„ÙŠØ© Ù„Ù„Ù…Ø§Ø³Ø­"""
    print("=" * 60)
    print("ğŸ” Anonymous Security Scanner - Pro 3.0")
    print("=" * 60)
    print("âš ï¸  For ethical and authorized testing only")
    print()
    
    while True:
        print("\nOptions:")
        print("1. Scan a website")
        print("2. View help")
        print("3. Exit")
        
        choice = input("\nSelect option (1-3): ").strip()
        
        if choice == '1':
            print("\n" + "=" * 40)
            print("WEBSITE SCAN")
            print("=" * 40)
            
            url = input("\nEnter website URL (e.g., https://example.com): ").strip()
            
            if not url.startswith(('http://', 'https://')):
                url = 'https://' + url
            
            print(f"\nTarget: {url}")
            print("Initializing anonymous scanner...")
            
            # Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† Ø§Ù„Ø¥Ø°Ù† (Ù„Ø£ØºØ±Ø§Ø¶ ØªØ¹Ù„ÙŠÙ…ÙŠØ©)
            print("\nâš ï¸  LEGAL DISCLAIMER:")
            print("1. You must have explicit permission to scan this website")
            print("2. Unauthorized scanning is illegal")
            print("3. This tool is for educational purposes only")
            
            confirm = input("\nDo you have permission to scan this website? (yes/no): ").strip().lower()
            
            if confirm not in ['yes', 'y']:
                print("âŒ Scan cancelled. Only scan websites you own or have permission to test.")
                continue
            
            print("\nğŸš€ Starting anonymous scan...")
            print("âš ï¸  Your identity is protected. No logs are stored.")
            print("â³ This may take a few minutes...")
            
            try:
                # Ø¥Ù†Ø´Ø§Ø¡ Ø§Ù„Ù…Ø§Ø³Ø­ Ø§Ù„Ù…Ø¬Ù‡ÙˆÙ„
                scanner = AnonymousScanner(url)
                
                # ØªØ´ØºÙŠÙ„ Ø§Ù„ÙØ­Øµ
                report = scanner.run_stealth_scan()
                
                # Ø¹Ø±Ø¶ Ø§Ù„Ù†ØªØ§Ø¦Ø¬
                print("\n" + "=" * 60)
                print("ğŸ“Š SCAN RESULTS")
                print("=" * 60)
                
                print(f"\nTarget: {report['scan_info']['target']}")
                print(f"Security Score: {report['summary']['security_score']}/100")
                print(f"Risk Level: {report['summary']['risk_level']}")
                print(f"Vulnerabilities Found: {report['summary']['total_vulnerabilities']}")
                
                if report['vulnerabilities']:
                    print("\nğŸ”´ VULNERABILITIES FOUND:")
                    for i, vuln in enumerate(report['vulnerabilities'], 1):
                        print(f"\n{i}. [{vuln['severity']}] {vuln['name']}")
                        print(f"   Description: {vuln['description']}")
                        print(f"   Fix: {vuln['fix'][:100]}...")
                else:
                    print("\nâœ… No critical vulnerabilities found!")
                
                # Ø­ÙØ¸ Ø§Ù„ØªÙ‚Ø±ÙŠØ±
                save = input("\nğŸ’¾ Save report to file? (yes/no): ").strip().lower()
                if save in ['yes', 'y']:
                    format_choice = input("Format (json/txt): ").strip().lower()
                    if format_choice in ['json', 'txt']:
                        filename = scanner.save_report(report, format_choice)
                        print(f"âœ… Report saved as: {filename}")
                    else:
                        print("âš ï¸  Invalid format, saving as JSON...")
                        filename = scanner.save_report(report, 'json')
                        print(f"âœ… Report saved as: {filename}")
                
                # Ø¹Ø±Ø¶ Ø§Ù„ØªÙˆØµÙŠØ§Øª
                if report['recommendations']:
                    print("\nğŸ’¡ RECOMMENDATIONS:")
                    for rec in report['recommendations']:
                        print(f"\nâ€¢ [{rec['priority']}] {rec['action']}")
                        print(f"  {rec['description']}")
                
                print("\n" + "=" * 60)
                print("ğŸ” Scan completed anonymously")
                print(f"Session ID: {report['scan_info']['session_id']}")
                print("All data is encrypted and temporary")
                print("=" * 60)
                
            except Exception as e:
                print(f"\nâŒ Scan failed: {str(e)[:100]}")
                print("Please check the URL and try again.")
        
        elif choice == '2':
            print("\n" + "=" * 40)
            print("HELP & INFORMATION")
            print("=" * 40)
            print("\nğŸ“Œ ABOUT THIS SCANNER:")
            print("â€¢ Completely anonymous - no IP logging")
            print("â€¢ Uses rotating proxies and User-Agents")
            print("â€¢ All data is encrypted")
            print("â€¢ No personal information stored")
            
            print("\nğŸ”’ SECURITY FEATURES:")
            print("â€¢ Random session IDs")
            print("â€¢ Encrypted logs")
            print("â€¢ Proxy rotation")
            print("â€¢ Request throttling")
            print("â€¢ No persistent storage")
            
            print("\nâš ï¸  LEGAL NOTICE:")
            print("â€¢ Only scan websites you own")
            print("â€¢ Get written permission first")
            print("â€¢ For educational purposes only")
            print("â€¢ Unauthorized scanning is illegal")
            
            print("\nğŸ” TESTS PERFORMED:")
            print("â€¢ SSL/TLS configuration")
            print("â€¢ Security headers")
            print("â€¢ SQL injection vulnerabilities")
            print("â€¢ Cross-site scripting (XSS)")
            print("â€¢ Sensitive file exposure")
            print("â€¢ Directory listing")
            print("â€¢ CMS detection")
        
        elif choice == '3':
            print("\nğŸ‘‹ Goodbye! Stay secure!")
            break
        
        else:
            print("\nâŒ Invalid option. Please try again.")

# ==================== Ø§Ù„Ø¯Ø§Ù„Ø© Ø§Ù„Ø±Ø¦ÙŠØ³ÙŠØ© ====================
def main():
    """Ø§Ù„Ø¯Ø§Ù„Ø© Ø§Ù„Ø±Ø¦ÙŠØ³ÙŠØ© Ù„Ù„ØªØ´ØºÙŠÙ„"""
    
    # Ø¹Ø±Ø¶ Ø´Ø¹Ø§Ø± Ø§Ù„Ù†Ø¸Ø§Ù…
    banner = """
    â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
    â•‘   ğŸ” Anonymous Security Scanner - Pro 3.0       â•‘
    â•‘                                                  â•‘
    â•‘  Features:                                       â•‘
    â•‘  â€¢ Complete anonymity                           â•‘
    â•‘  â€¢ No IP logging                                â•‘
    â•‘  â€¢ Encrypted sessions                           â•‘
    â•‘  â€¢ Proxy rotation                               â•‘
    â•‘  â€¢ Legal compliance checker                     â•‘
    â•‘                                                  â•‘
    â•‘  âš ï¸  For authorized testing only                â•‘
    â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    """
    
    print(banner)
    
    # Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† Ø§Ù„ØªÙˆØ§ÙÙ‚
    try:
        import requests
        import cryptography
        print("âœ… All dependencies are available")
    except ImportError as e:
        print(f"âŒ Missing dependency: {e}")
        print("Installing required packages...")
        
        # Ù…Ø­Ø§ÙˆÙ„Ø© Ø§Ù„ØªØ«Ø¨ÙŠØª Ø§Ù„ØªÙ„Ù‚Ø§Ø¦ÙŠ
        try:
            import subprocess
            subprocess.check_call([sys.executable, "-m", "pip", "install", "requests", "cryptography", "beautifulsoup4"])
            print("âœ… Dependencies installed successfully")
        except:
            print("âŒ Could not install dependencies automatically")
            print("Please install manually: pip install requests cryptography beautifulsoup4")
            return
    
    # Ø¨Ø¯Ø¡ Ø§Ù„ÙˆØ§Ø¬Ù‡Ø© Ø§Ù„ØªÙØ§Ø¹Ù„ÙŠØ©
    interactive_scanner()

