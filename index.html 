    def check_ssl_tls(self):
        """ÙØ­Øµ Ø¥Ø¹Ø¯Ø§Ø¯Ø§Øª SSL/TLS"""
        self.log_encrypted("ğŸ” Checking SSL/TLS configuration...")
        
