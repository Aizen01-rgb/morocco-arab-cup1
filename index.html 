                        self.vulnerabilities.append({
                            'id': 'SSL-EXPIRING-SOON',
                            'name': 'SSL Certificate Expiring Soon',
                            'severity': 'MEDIUM',
                            'description': 'Ø´Ù‡Ø§Ø¯Ø© SSL Ø¹Ù„Ù‰ ÙˆØ´Ùƒ Ø§Ù„Ø§Ù†ØªÙ‡Ø§Ø¡',
                            'details': f'Expires in {days_remaining} days',
                            'fix': 'Renew SSL certificate',
                            'impact': 'Website security warnings for users'
                        })
                    
