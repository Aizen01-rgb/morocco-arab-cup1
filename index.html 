            if not response:
                return None
            
            headers = response.headers
            
            security_headers = [
                'X-Frame-Options',
                'Content-Security-Policy',
                'Strict-Transport-Security',
                'X-Content-Type-Options',
                'Referrer-Policy',
                'Permissions-Policy'
            ]
            
            missing_headers = []
            for header in security_headers:
                if header not in headers:
                    missing_headers.append(header)
            
            if missing_headers:
                self.vulnerabilities.append({
                    'id': 'SEC-HEADERS-MISSING',
                    'name': 'Missing Security Headers',
                    'severity': 'MEDIUM',
                    'description': 'Ø±Ø¤ÙˆØ³ Ø£Ù…Ø§Ù† Ø£Ø³Ø§Ø³ÙŠØ© Ù…ÙÙ‚ÙˆØ¯Ø©',
                    'details': f'Missing: {", ".join(missing_headers)}',
                    'fix': 'Add security headers to server configuration',
                    'impact': 'Increased risk of XSS, clickjacking, and other attacks'
                })
            
            self.log_encrypted("âœ… Security headers check completed")
            return response
            
        except Exception as e:
            self.log_encrypted(f"âŒ Error checking headers: {str(e)[:50]}")
            return None
    
    def check_sql_injection(self):
