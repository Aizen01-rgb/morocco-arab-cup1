                self.log_encrypted(f"Proxy failed, retrying... ({attempt + 1}/{max_retries})")
                continue
            except requests.exceptions.Timeout:
                self.log_encrypted(f"Timeout, retrying... ({attempt + 1}/{max_retries})")
                continue
            except Exception as e:
                self.log_encrypted(f"Request error: {str(e)[:50]}...")
                continue
        
        return None
    
    def log_encrypted(self, message):
