    def extract_forms(self):
        """Ø§Ø³ØªØ®Ø±Ø§Ø¬ Ø§Ù„Ù†Ù…Ø§Ø°Ø¬ Ù…Ù† Ø§Ù„ØµÙØ­Ø©"""
        forms = []
        
        try:
            response = self.make_request(self.target_url)
            if not response:
                return forms
            
            soup = BeautifulSoup(response.text, 'html.parser')
            
            for form in soup.find_all('form'):
                form_info = {
                    'action': urljoin(self.target_url, form.get('action', self.target_url)),
                    'method': form.get('method', 'GET').upper(),
                    'inputs': []
                }
                
                for input_tag in form.find_all(['input', 'textarea', 'select']):
                    input_info = {
                        'name': input_tag.get('name', ''),
                        'type': input_tag.get('type', 'text'),
                        'value': input_tag.get('value', ''),
                        'tag': input_tag.name
                    }
                    
                    if input_tag.name == 'select':
                        input_info['options'] = [opt.get('value') for opt in input_tag.find_all('option')]
                    
                    form_info['inputs'].append(input_info)
                
                forms.append(form_info)
            
        except Exception as e:
            self.log_encrypted(f"âŒ Form extraction error: {str(e)[:50]}")
        
        return forms
    
